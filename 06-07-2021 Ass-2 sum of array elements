import java.util.*;
class Main 
{
   public static void main(String args[])
   {
       int i,n;
      int arr[]={1,2,3,4,5};
      int sum=0;
      {
         for(int num:arr)
         {
           sum=sum+num;
           }
      System.out.println("sum of array elements is"+sum);
   }
}
}

output:
sum of array elements is15
