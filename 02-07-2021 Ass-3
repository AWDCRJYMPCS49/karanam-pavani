import java.util.*;
class Main
{
    public static void main(String args[])
{
      int n,fact=1,i;
      Scanner sc=new Scanner(System.in);
      n=sc.nextInt();
        for(i=1;i<=n;i++)
        {
            fact*=i;
        }
        System.out.printf("fact of %d = %d",n,fact);
    }
}

output:
fact of 3 = 6

