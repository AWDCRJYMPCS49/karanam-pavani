import java.util.*;
class Main
{
    public static void main(String args[]) 
    {
        int n;
        Scanner sc = new Scanner(System.in);
        n=sc.nextInt();
        if(n%4==0)
        {
            System.out.println(++n);
        }
        else
        {
            System.out.print(--n);
        }
    }
}

output:
enter a number
10

9

